{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Productos",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "text",
      "required": true
    },
    "precio": {
      "type": "decimal",
      "required": true,
      "min": 1
    },
    "stock_general": {
      "type": "integer",
      "default": 0,
      "required": false,
      "min": 0
    },
    "imagenes": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "SKU": {
      "type": "string"
    },
    "fecha_creacion": {
      "type": "datetime"
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "Publicado",
        "Borrador",
        "Interno"
      ],
      "required": true,
      "default": "Interno"
    },
    "etiquetas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::etiqueta.etiqueta",
      "mappedBy": "productos"
    },
    "categorias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::categoria.categoria",
      "mappedBy": "productos"
    },
    "regalo": {
      "type": "boolean",
      "required": false
    },
    "parche": {
      "type": "boolean",
      "required": false
    },
    "personalizacion": {
      "type": "boolean",
      "required": false
    },
    "stock_104": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_116": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_128": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_140": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_152": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_164": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_xs": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_s": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
