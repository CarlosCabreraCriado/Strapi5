    "stock_l": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_xl": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_2xl": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_3xl": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "stock_4xl": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "required": true
    },
    "imagen_portada": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Temporada": {
      "type": "enumeration",
      "enum": [
        "Temporada 2024 / 2025",
        "Temporada 2023 / 2024",
        "Temporada 2022 / 2023"
      ],
      "required": true,
      "default": "Temporada 2024 / 2025"
    },
    "stock": {
      "type": "dynamiczone",
      "components": [
        "sin-tallas.sin-tallas",
        "tallas-ropa.corte-unisex",
        "tallas-ropa.corte-masculino",
        "tallas-ropa.corte-femenino"
      ],
      "required": true,
      "min": 1
    }
  }
